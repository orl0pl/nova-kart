{
    "manifest_version": 2,
    "name": "NovaKart",
    "version": "0.1.0",
    "description": "A simple extension to override new tab page, built with Svelte",
    "permissions": ["storage"],
    "icons": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png",
      "128": "icons/icon_128.png"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    }
  }
  